{
  "name": "Disconnect Privacy Icons",
  "version": "1.0.22",
  "manifest_version": 2,
  "description": "Privacy policies are way too complicated. We've simplified them for you.",
  "options_page": "markup/custom.html",
  "icons": {
    "128": "images/128.png",
    "48": "images/48.png",
    "16": "images/16.png"
  },
  "homepage_url": "https://disconnect.me",
  "permissions": [
    "chrome://favicon/",
    "http://*/*",
    "https://*/*",
    "contextMenus",
    "idle",
    "notifications",
    "webNavigation",
    "tabs"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "background": {
    "scripts": [
      "scripts/vendor/jquery/jquery.js",
      "scripts/background.js"
    ]
  },
  "browser_action": {
    "default_title": "Disconnect Privacy Icons",
    "default_icon": "images/48.png",
    "default_popup": "markup/popup.html"
  },
  "web_accessible_resources": [
    "images/*",
    "fonts/*"
  ]
}